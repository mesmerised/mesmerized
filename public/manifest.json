{
  "manifest_version": 2,
  "author": "Emad Alam <emad.alam@yahoo.com>",
  "name": "__MSG_extName__",
  "short_name": "__MSG_extName__",
  "description": "__MSG_extDesc__",
  "version": "0.2.0",
  "default_locale": "en",
  "homepage_url": "https://mesmerized.me",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "tabpage.html"
  },
  "options_ui": {
    "chrome_style": false,
    "page": "options.html"
  },
  "content_security_policy": "script-src 'self' https://api.forismatic.com; object-src 'self'",
  "permissions": [
    "geolocation",
    "alarms",
    "notifications",
    "storage",
    "unlimitedStorage",
    "http://*.mesmerized.me/*"
  ]
}
